{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "node",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "program": "${file}",
      "autoAttachChildProcesses": true,
      "sourceMaps": false,
      "smartStep": true,
      "skipFiles": [
        // force-array-multiline
        // "node_modules/**",
        "node_modules/@babel/template/**",
        "node_modules/@dmail/cancellation/**",
        "<node_internals>/**/*.js"
      ]
    },
  ]
}
